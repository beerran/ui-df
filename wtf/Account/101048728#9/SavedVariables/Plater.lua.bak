						["Constructor"] = "function (self, unitId, unitFrame, envTable)\n    -- used for nameColouring\n    -- AARRGGBB\n    local markerToHex = {\n       -- [1] = \"FFEAEA0D\", -- Yellow 5 Point Star\n       -- [2] = \"FFEAB10D\", -- Orange Circle\n       -- [3] = \"FFCD00FF\", -- Purple Diamond\n       -- [4] = \"FF06D425\", -- Green Triangle\n       -- [5] = \"FFB3E3D8\", -- Light Blue Moon\n       -- [6] = \"FF0CD2EA\", -- Blue Square\n       -- [7] = \"FFD6210B\", -- Red Cross\n       -- [8] = \"FFFFFFFF\", -- White Skull\n        \n        [1] = \"FFFFFFFF\", -- name colors disable\n        [2] = \"FFFFFFFF\", -- name colors disable\n        [3] = \"FFFFFFFF\", -- name colors disable\n        [4] = \"FFFFFFFF\", -- name colors disable\n        [5] = \"FFFFFFFF\", -- name colors disable\n        [6] = \"FFFFFFFF\", -- name colors disable\n        [7] = \"FFFFFFFF\", -- name colors disable\n        [8] = \"FFFFFFFF\", -- name colors disable\n    }\n    \n    -- Makes it so you take their first name e.g Jessie Howlis -> Jessie\n    -- Old way was some degen fucking shit this is easier\n    local nameBlacklist = {\n        [\"the\"] = true,\n        [\"of\"] = true,\n        [\"Tentacle\"] = true,\n        [\"Apprentice\"] = true,  \n        [\"Denizen\"] = true,\n        [\"Emissary\"] = true,\n        [\"Howlis\"] = true, \n        [\"Terror\"] = true, \n        [\"Totem\"] = true, \n        [\"Waycrest\"] = true,\n        [\"Dummy\"] = true, -- Testing Purposes\n        [\"Aspect\"] = true\n    }\n    \n    \n    -- Override for names, Thanks to Nnoggie for all of the dungeon ones\n    local renameTable = {\n        -- Testing\n        [\"Fleshripper Vulture\"] = \"Shit Bird\",\n        [\"Roaming Nibbler\"] = \"Roaming Saurid\",\n        -- Grand Delusions\n        [\"Thing From Beyond\"] = \"Ion Hazzikostas\",\n        -- The Eternal Palace\n        [\"Abyssal Commander Sivara\"] = \"Sivara\",\n        [\"Blackwater Behemoth\"] = \"Behemoth\",\n        [\"Radiance of Azshara\"] = \"Radiance\",\n        [\"Lady Ashvane\"] = \"Ashvane\",\n        [\"Orgozoa\"] = \"Orgozoa\",\n        [\"Silivaz the Zealous\"] = \"Silivaz\",\n        [\"Pashmar the Fanatical\"] = \"Pashmar\",\n        [\"Za'qul, Harbinger of Ny'alotha\"] = \"Za'qul\",\n        [\"Queen Azshara\"] = \"Azshara\",\n        [\"Thing From Beyond\"] = \"Ion Hazzikostas\",\n        --Awakened (BfA S4 Affix)\n        [\"Voidweaver Mal'thir\"] = \"Voidweaver Mal'thir\",\n        [\"Samh'rek, Beckoner of Chaos\"] = \"Samh'rek\",\n        [\"Urg'roth, Breaker of Heroes\"] = \"Urg'roth\",\n        [\"Blood of the Corruptor\"] = \"Blood of the Corruptor\",\n        --Atal'Dazar\n        [\"Reanimated Honor Guard\"] = \"Honor Guard\",\n        [\"Dinomancer Kish'o\"] = \"Dinomancer\",\n        [\"Dazar'ai Honor Guard\"] = \"Honor Guard\",\n        [\"Shieldbearer of Zul\"] = \"Shieldbearer\",\n        --Freehold\n        [\"Cutwater Knife Juggler\"] = \"Knife Juggler\",\n        [\"Skycap'n Kragg\"] = \"Skycap'n Kragg\",\n        [\"Harlan Sweete\"] = \"Harlan Sweete\",\n        --King's Rest\n        [\"Minion of Zul\"] = \"Minion\",\n        [\"Shadow-Borne Witch Doctor\"] = \"Witch Doctor\",\n        [\"Queen Wasi\"] = \"Queen\",\n        [\"King Timalji\"] = \"King\",\n        [\"Queen Patlaa\"] = \"Queen\",\n        [\"King A'akul\"] = \"King\",\n        [\"Shieldbearer of Zul\"] = \"Shieldbearer\",\n        [\"Seneschal M'bara\"] = \"Seneschal M'bara\",\n        [\"King Rahu'ai\"] = \"King\",\n        [\"Guard Captain Atu\"] = \"Guard Captain\",\n        [\"Mchimba the Embalmer\"] = \"Mchimba\",\n        [\"Spectral Hex Priest\"] = \"Hex Priest\",\n        [\"The Golden Serpent\"] = \"The Golden Serpent\",\n        [\"Kula the Butcher\"] = \"Kula the Butcher\",\n        [\"Zanazal the Wise\"] = \"Zanazal the Wise\",\n        [\"Aka'ali the Conqueror\"] = \"Aka'ali the Conqueror\",\n        [\"King Dazar\"] = \"King Dazar\",\n        --Shrine of the Storm\n        [\